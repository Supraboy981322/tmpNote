.{
    //server options
    .server = .{
        //listening port
        .port = 8795,
        //log settings
        .log = .{
            .file = "log.json",
        },
    },

    //note settings
    .notes = .{
        //maximum note size
        .max_size = "10MB",
        //compression type ('.brotli', '.zlib', '.gzip', or '.none')
        .compression = .brotli,
        //encrypt notes
        .use_encryption = true,
    },
}
